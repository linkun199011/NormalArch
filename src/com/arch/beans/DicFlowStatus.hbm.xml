<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.arch.beans">
	<class name="DicFlowStatus" table="dic_flow_status">
		<id name="flowStatusId" type="java.lang.Integer" column="flow_status_id" />
		<property name="flowStatus" column="flow_status" type="java.lang.String"/>			
	</class>
</hibernate-mapping>
