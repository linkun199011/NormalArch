<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.arch.beans">
	<class name="TbSeqNum" table="tb_seq_num">
		<id name="tbName" type="java.lang.String" column="tb_name" />
		<property name="seqNextVal" type="java.lang.Integer" column="seq_next_val" />
		
	</class>
</hibernate-mapping>
